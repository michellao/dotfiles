{
    "telemetry.telemetryLevel": "off",
    "editor.fontFamily": "'FreeMono', 'Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
    "workbench.enableExperiments": false,
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c17",
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#8aceff",
        "editorBracketHighlight.foreground2": "#c32be4",
        "editorBracketHighlight.foreground3": "#FF297A",
        "editorBracketHighlight.foreground4": "#3FE086",
        "editorBracketHighlight.foreground5": "#daf846",
        "editorBracketHighlight.unexpectedBracket.foreground": "#8256ef"
    },
    "update.mode": "none",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "remote.autoForwardPorts": false,
    "remote.SSH.useExecServer": false,
    "thunder-client.httpVersion": "HTTP/2",
    "thunder-client.indentSize": 4,
    "thunder-client.responseLimit": 5,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.fontFamily": "monospace",
    "window.customTitleBarVisibility": "auto",
    "window.titleBarStyle": "custom",
    "editor.stickyScroll.enabled": false
}
